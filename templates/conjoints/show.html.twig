{% extends 'base.html.twig' %}

{% block title %}Conjoints{% endblock %}

{% block body %}
    <h1>Conjoints</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ conjoint.id }}</td>
            </tr>
            <tr>
                <th>Pseudo</th>
                <td>{{ conjoint.pseudo }}</td>
            </tr>
            <tr>
                <th>Age</th>
                <td>{{ conjoint.age }}</td>
            </tr>
            <tr>
                <th>Sexe</th>
                <td>{{ conjoint.sexe }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ conjoint.description }}</td>
            </tr>
            <tr>
                <th>Img</th>
                <img src=" {{ asset('uploads/' ~ conjoint.img) }} " alt="">
            </tr>
            <tr>
                <th>Style</th>
                <td>{{ conjoint.style }}</td>
            </tr>
            <tr>
                <th>Categorie</th>
                <td>{{ conjoint.categorie }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_conjoints_index') }}">back to list</a>

    <a href="{{ path('app_conjoints_edit', {'id': conjoint.id}) }}">edit</a>

    {{ include('conjoints/_delete_form.html.twig') }}
{% endblock %}
